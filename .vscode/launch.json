{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug iOS Hermes",
      "request": "launch",
      "type": "reactnativedirect",
      "cwd": "${workspaceFolder}",
      "platform": "ios"
    },
    {
      "name": "Attach to Direct iOS - Experimental",
      "request": "attach",
      "type": "reactnativedirect",
      "cwd": "${workspaceFolder}",
      "platform": "ios",
      "useHermesEngine": false,
      "port": 9221
    },
    {
      "name": "Debug en iOS",
      "type": "reactnative",
      "request": "launch",
      "platform": "ios",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug en Android",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/App.tsx",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
